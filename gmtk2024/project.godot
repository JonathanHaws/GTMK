config_version=5

[Fmod]

General/auto_initialize=true
General/channel_count=1024
General/is_live_update_enabled=true
General/is_memory_tracking_enabled=false
"Software Format/sample_rate"=48000
"Software Format/speaker_mode"=3
"Software Format/raw_speaker_count"=0
General/default_listener_count=1
General/banks_path="res://sound"
General/should_load_by_name=false
DSP/dsp_buffer_size=512
DSP/dsp_buffer_count=4
"3D Settings/doppler_scale"=1.0
"3D Settings/distance_factor"=1.0
"3D Settings/rolloff_scale"=1.0

[application]

config/name="ROBOSAVIOR"
config/tags=PackedStringArray("jam")
run/main_scene="res://nodes/main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0.0156863, 0.0313726, 0.0352941, 1)
boot_splash/image="res://logo.png"
config/icon="res://icon.png"
config/windows_native_icon="res://favicon.ico"

[audio]

buses/default_bus_layout=""

[autoload]

GlobalFMODBankLoader="*res://nodes/globals/fmod_bank_loader.tscn"
ScaleManager="*res://nodes/globals/ScaleController.gd"
ScoreController="*res://nodes/systems/ScoreController.gd"
FmodManager="*res://addons/fmod/FmodManager.gd"

[display]

window/size/viewport_width=1200
window/size/viewport_height=640

[editor_plugins]

enabled=PackedStringArray("res://addons/fmod/plugin.cfg")

[global_group]

